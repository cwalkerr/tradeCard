<nav aria-label="Page navigation">
  <ul class="pagination d-flex justify-content-center my-4">
    <% if (currentPage > 1) { %>
    <li class="page-item">
      <a class="page-link" href="?<%= new URLSearchParams({ ...checkedBoxes, page: currentPage - 1 }).toString() %>">Previous</a>
    </li>
    <% } %> <% for (let i = startPage; i <= endPage; i++) { %>
    <li class="page-item <%= currentPage === i ? 'active' : '' %>">
      <a class="page-link" href="?<%= new URLSearchParams({ ...checkedBoxes, page: i }).toString() %>"><%= i %></a>
    </li>
    <% } %> <% if (currentPage < totalPages) { %>
    <li class="page-item">
      <a class="page-link" href="?<%= new URLSearchParams({ ...checkedBoxes, page: currentPage + 1 }).toString() %>">Next</a>
    </li>
    <% } %>
  </ul>
</nav>