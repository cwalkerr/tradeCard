                  <label class="form-check-label font-monospace fw-semibold" for="rarityCheck<%=i%>">
                    <%= rarities[i] %>
                  </label>
                </div>
              </div>
              <% } %>
            </div>
          </div>
        </div>
      </div>
      <div class="my-4">
