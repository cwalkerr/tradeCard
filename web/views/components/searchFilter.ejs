<script>
  // HP
  const minHp = document.getElementById('minHp');
  const maxHp = document.getElementById('maxHp');
  const minValue = document.querySelector('.minValue');
  const maxValue = document.querySelector('.maxValue');

  minHp.addEventListener('input', () => {
    minValue.textContent = `Minimum: ${minHp.value}`;
  });

  maxHp.addEventListener('input', () => {
    maxValue.textContent = `Maximum: ${maxHp.value}`;
  });

  minHp.addEventListener('change', () => {
    if (parseInt(minHp.value) > parseInt(maxHp.value)) {
      maxHp.value = minHp.value;
      maxValue.textContent = `Maximum: ${maxHp.value}`;
    }
  });

  maxHp.addEventListener('change', () => {
    if (parseInt(maxHp.value) < parseInt(minHp.value)) {
      minHp.value = maxHp.value;
      minValue.textContent = `Minimum: ${minHp.value}`;
    }
  });

